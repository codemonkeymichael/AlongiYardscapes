@model alongiYardscapes.Models.GalleryImages

@{
    ViewData["Title"] = "Gallery";
}

<h1>Gallery</h1>
<div class="slider">
    @foreach (var img in Model.Images)
    {
        <div>
            <img class="slider-image" src="@img.Thumb" />
        </div>
    }
</div>




@section Scripts{
    <script type="text/javascript">
        $(function () {
            AY.Gallery.Init();
        });
    </script>
}
